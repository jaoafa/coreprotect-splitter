{
  "name": "coreprotect-splitter",
  "version": "1.0.0",
  "description": "Split old CoreProtect data into multiple tables.",
  "main": "dist/main.js",
  "repository": "git@github.com:jaoafa/coreprotect-splitter.git",
  "author": "Tomachi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "package": "run-s compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@types/cli-progress": "3.11.0",
    "@types/node": "18.16.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vercel/ncc": "0.36.1",
    "cli-progress": "3.12.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "46.0.0",
    "mysql2": "3.2.4",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4",
    "yarn-run-all": "3.1.1"
  }
}
